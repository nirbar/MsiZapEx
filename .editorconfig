# top-most EditorConfig file
root = true

# Windows-style newlines with a newline ending every file
[*]
end_of_line = crlf
insert_final_newline = true

# XML- 2 space indentation
[*.*proj,props,targets,xaml,nuspec]
indent_style = space
indent_size = 2
charset = utf-8

# CS- 4 space indentation
[*.cs]
indent_style = space
indent_size = 4
charset = utf-8
